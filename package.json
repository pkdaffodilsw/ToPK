{
  "name": "toothie",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "adb:input-text": "adb shell input text",
    "adb:toggle-dev-menu": "adb shell input keyevent 82",
    "android:emulator": "bash ./scripts/launch-android-emulator.sh",
    "android:studio": "open -a '/Applications/Android Studio.app' android",
    "clean:all": "yarn clean:caches && git clean -xdn && echo 'Run: \"git clean -xdf\" to continue'",
    "clean:build": "rimraf android/.gradle android/.idea android/app/app.iml android/app/build ios/build",
    "clean:caches": "watchman watch-del-all && rimraf $TMPDIR/react-native-packager-cache-* $TMPDIR/metro-bundler-cache-*",
    "format": "prettier --write '{source,tests}/**/*.js'",
    "lint": "eslint . --fix",
    "postinstall": "env-cmd .env bash ./scripts/postinstall.sh && patch-package && npx jetify -r",
    "reset-dependencies": "yarn clean:caches && yarn clean:build && rimraf node_modules ios/Pods && yarn && cd ios && pod install && cd ..",
    "start": "node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:question-tree": "jest --watch ./question-tree/*.spec.js",
    "xcode": "open ios/Toothie.xcworkspace"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.6.1",
    "@react-native-community/datetimepicker": "2.1.0",
    "@react-native-community/viewpager": "1.1.7",
    "appcenter": "3.0.0",
    "appcenter-analytics": "3.0.0",
    "appcenter-crashes": "3.0.0",
    "buffer": "5.4.3",
    "chroma-js": "2.0.6",
    "compare-versions": "3.5.1",
    "date-fns": "2.6.0",
    "enroll": "3.2.0",
    "events": "3.0.0",
    "expo-av": "7.0.1",
    "expo-image-manipulator": "7.0.0",
    "expo-image-picker": "7.0.0",
    "expo-keep-awake": "7.0.0",
    "formik": "1.5.8",
    "gullwing": "3.0.1",
    "https-browserify": "1.0.0",
    "jsc-android": "241213.1.0",
    "json-schema-ref-parser": "7.1.2",
    "jwt-decode": "2.2.0",
    "libphonenumber-js": "1.7.25",
    "lodash.debounce": "4.0.8",
    "lodash.get": "4.4.2",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.2",
    "lodash.set": "4.3.2",
    "lodash.setwith": "4.3.2",
    "lodash.unset": "4.5.2",
    "mime-types": "2.1.24",
    "path-browserify": "1.0.0",
    "personnummer.js": "1.1.7",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-camera": "2.11.2",
    "react-native-device-info": "3.1.4",
    "react-native-fbsdk": "0.10.3",
    "react-native-firebase": "5.5.6",
    "react-native-gesture-handler": "1.3.0",
    "react-native-localize": "1.2.1",
    "react-native-markdown-display": "5.1.0",
    "react-native-modal": "11.5.3",
    "react-native-modal-datetime-picker": "8.x.x",
    "react-native-notifications": "2.1.5",
    "react-native-safe-area-view": "0.14.7",
    "react-native-screens": "1.0.0-alpha.22",
    "react-native-splash-screen": "3.2.0",
    "react-native-twilio-video-webrtc": "git+https://github.com/blackuy/react-native-twilio-video-webrtc#0255af5db9d28909dbb1277a2c566bae5a3cf124",
    "react-native-unimodules": "0.6.0",
    "react-native-webview": "7.6.0",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.7.3",
    "react-navigation-tabs": "1.2.0",
    "rn-viewpager": "git+https://github.com/buildcom/React-Native-ViewPager#426a858fc4cb4321f6b6d9916c84e5db7f6d5477",
    "stream-http": "3.1.0",
    "tipsi-stripe": "8.0.0-beta.7",
    "url": "0.11.0",
    "uuid": "3.3.3",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@prettier/plugin-xml": "0.3.0",
    "ajv": "6.10.2",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "cheerio": "1.0.0-rc.3",
    "env-cmd": "8.0.2",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-flowtype": "3.12.2",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-react-native": "3.7.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "jetifier": "1.6.4",
    "lint-staged": "9.2.1",
    "metro-react-native-babel-preset": "0.55.0",
    "patch-package": "6.2.0",
    "postinstall-postinstall": "git+https://github.com/osrl/postinstall-postinstall#0c769263f79689c46f29698708980d239d99334e",
    "prettier": "1.19.1",
    "react-native-rename": "git+https://intelleqt.visualstudio.com/Tand24/_git/ReactNativeRename#9315417b86e40857d841489a5b7a3a2f6536f6f1",
    "react-test-renderer": "16.8.3",
    "rimraf": "3.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "/node_modules/(?!(jest-)?react-native|tipsi-stripe)/.+"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/mocks/@react-native-community/async-storage.js",
      "<rootDir>/mocks/react-native-device-info.js",
      "<rootDir>/mocks/react-native-localize.js",
      "<rootDir>/mocks/tipsi-stripe.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "prettier --write '{source,tests}/**/*.js'",
      "git add"
    ]
  }
}
